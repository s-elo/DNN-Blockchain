* {
  margin: 0;
  padding: 0;
  font-size: 16px;
  box-sizing: border-box;
}
.app-body .app-content {
  width: 100vw;
  min-height: 100vh;
  overflow-y: auto;
  padding: 40px 0;
  background-color: #a19c9c;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.app-body .app-content .demo-name {
  font-weight: bold;
  font-size: 18px;
}
